{
  ".source.json": {
    "Atom Snippet": {
      "prefix": "snip",
      "body": "{\n  \"${1:.source.js}\": {\n    \"${2:Snippet Name}\": {\n      \"prefix\": \"${3:hello}\",\n      \"body\": \"${4:Hello World!}\"\n    }\n  }\n}$5"
    },
    "Atom Keymap": {
      "prefix": "key",
      "body": "{\n  \"${1:body}\": {\n    \"${2:cmd}-${3:i}\": \"${4:namespace}:${5:event}\"\n  }\n}$6"
    }
  },
  ".source.coffee": {
    "Atom Snippet": {
      "prefix": "snip",
      "body": "'${1:.source.js}':\n  '${2:Snippet Name}':\n    'prefix': '${3:hello}'\n    'body': '${4:Hello World!}'$5"
    },
    "Atom Keymap": {
      "prefix": "key",
      "body": "'${1:body}':\n  '${2:cmd}-${3:i}': '${4:namespace}:${5:event}'$6"
    }
  }
}
